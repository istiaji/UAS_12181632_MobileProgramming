<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.co.imastudio.santri.uas_recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Explore Gorontalo"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="id.co.imastudio.santri.uas_recyclerview.SplashActivity"
            android:label="Explore Gorontalo"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="id.co.imastudio.santri.uas_recyclerview.MainActivity"
            android:label="Explore Gorontalo" />
        <activity
            android:name="id.co.imastudio.santri.uas_recyclerview.Main3Activity"
            android:label="Daftar Tempat Wisata" />
        <activity
            android:name="id.co.imastudio.santri.uas_recyclerview.DetailWisata"
            android:label="Detail Tempat Wisata"
            android:parentActivityName="id.co.imastudio.santri.uas_recyclerview.Main3Activity" />

    </application>

</manifest>